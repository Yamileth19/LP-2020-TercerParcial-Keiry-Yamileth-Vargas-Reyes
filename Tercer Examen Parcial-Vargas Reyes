	public void actividad1()
	{
		//10. Programa que determine cuál de tres números diferentes es el mayor, y cuáles el menor. (Si los números son iguales, enviar mensaje) (Usar IF)
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 1                             +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int n1, n2, n3;
		Console.WriteLine("Ingrese el primer numero: ");
		n1 = Int32.Parse(Console.ReadLine());
		Console.WriteLine("Ingrese el segundo numero: ");
		n2 = Int32.Parse(Console.ReadLine());
		Console.WriteLine("Ingrese el tercer numero: ");
		n3 = Int32.Parse(Console.ReadLine());
		if(n1==n2 && n1==n3){
			Console.WriteLine("Los numeros ingresados son iguales");
		}else{
			if(n1>n2 && n1>n3){
				Console.WriteLine("El numero: "+n1 + " es el mayor");
				if(n2>n3){
					Console.WriteLine("El numero: "+n3 + " es el menor");
				}else{
					Console.WriteLine("El numero: "+n2 + " es el menor");
				}
			}
			if(n2>n1 && n2>n3){
				Console.WriteLine("El numero: "+n2 + " es el mayor");
				if(n1>n3){
					Console.WriteLine("El numero: "+n3 + " es el menor");
				}else{
					Console.WriteLine("El numero: "+n1 + " es el menor");
				}
			}
			if(n3>n1 && n3>n2){
				Console.WriteLine("El numero: "+n3 + " es el mayor");
				if(n1>n2){
					Console.WriteLine("El numero: "+n2 + " es el menor");
				}else{
					Console.WriteLine("El numero: "+n1 + " es el menor");
				}
			}
		}
	}
	public void actividad2()
	{
		//11. Programa para leer 5 números, obtener su promedio, el mayor y el menor de esos números.
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 2                             +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int m1, m2, m3, m4, m5, m6=0, m7=0;
		double pro=0;
		Console.WriteLine("Ingrese cinco numeros: ");
		m1 = Int32.Parse(Console.ReadLine());
		m2 = Int32.Parse(Console.ReadLine());
		m3 = Int32.Parse(Console.ReadLine());
		m4 = Int32.Parse(Console.ReadLine());
		m5 = Int32.Parse(Console.ReadLine());
		pro = (m1+m2+m3+m4+m5)/5;
		Console.WriteLine("El promedio de los 5 números es de: "+pro);
		if(m1==m2 && m1==m3 && m1==m4 && m1==m5){
			Console.WriteLine("Los numeros ingresados son iguales");
		}else{
			if(m1>=m2 && m1>=m3 && m1>=m4 && m1>=m5 && m6<1){
				Console.WriteLine("El numero: "+m1 + " es el mayor");
				m6++;
			}
			if(m2>=m1 && m2>=m3 && m2>=m4 && m2>=m5 && m6<1){
				Console.WriteLine("El numero: "+m2 + " es el mayor");
				m6++;
			}
			if(m3>=m2 && m3>=m1 && m3>=m4 && m3>=m5 && m6<1){
				Console.WriteLine("El numero: "+m3 + " es el mayor");
				m6++;
			}
			if(m4>=m2 && m4>=m3 && m4>=m1 && m4>=m5 && m6<1){
				Console.WriteLine("El numero: "+m4 + " es el mayor");
				m6++;
			}
			if(m5>=m2 && m5>=m3 && m5>=m4 && m5>=m1 && m6<1){
				Console.WriteLine("El numero: "+m5 + " es el mayor");
				m6++;
			}
			if(m1<=m2 && m1<=m3 && m1<=m4 && m1<=m5 && m7<1){
				Console.WriteLine("El numero: "+m1 + " es el menor");
				m7++;
			}
			if(m2<=m1 && m2<=m3 && m2<=m4 && m2<=m5 && m7<1){
				Console.WriteLine("El numero: "+m2 + " es el menor");
				m7++;
			}
			if(m3<m2 && m3<m1 && m3<m4 && m3<m5 && m7<1){
				Console.WriteLine("El numero: "+m3 + " es el menor");
				m7++;
			}
			if(m4<m2 && m4<m3 && m4<m1 && m4<m5 && m7<1){
				Console.WriteLine("El numero: "+m4 + " es el menor");
				m7++;
			}
			if(m5<m2 && m5<m3 && m5<m4 && m5<m1 && m7<1){
				Console.WriteLine("El numero: "+m5 + " es el menor");
				m7++;
			}
		}
	}
	public void actividad3()
	{
		//12. Programa para elevar un número “X” a una potencia “n”
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 3                             +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int p1=0,p2=0;
		Console.WriteLine("Ingrese un números: ");
		p1 = Int32.Parse(Console.ReadLine());
		Console.WriteLine("Ingrese la potencia a la que quiere elevar el primer número: ");
		p2 = Int32.Parse(Console.ReadLine());
		Console.Write("El numero: "+ p1 +" con exponente " + p2 + " es igual a: ");
		Console.Write( Math.Pow (p1,p2));
	}
	public void actividad4()
	{
		/*2. Programa para sumar, restar, multiplicar y dividir dos números cualquiera. 
		(Mostrar los resultados de la operación seleccionada usando Switch)*/
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 4                             +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine(" ");
		string operacion = "";
		int numero1;
		int numero2;
		int oper;
		Console.WriteLine("Realicemos Operaciones");
		Console.WriteLine("< suma >    < resta >    < multiplicacion >    < division >");
		Console.WriteLine(" ");
		Console.WriteLine("Ingrese un número: ");
		numero1 = Int32.Parse(Console.ReadLine());
		
		Console.WriteLine("Ingrese otro número: ");
		numero2 = Int32.Parse(Console.ReadLine());
		
		Console.WriteLine("Escribe la operacion que deseas realizar");
		operacion = Console.ReadLine();
        
        switch(operacion)
		{
			case "suma":
				oper = numero1 + numero2 ;
                Console.WriteLine("La suma de "+ numero1 +" mas " + numero2 +" es igual a " + oper);
				break;
			case "resta":
                oper = numero1 - numero2 ;
                Console.WriteLine("La resta de "+ numero1 +" menos " + numero2 +" es igual a " + oper);
				break;
		    case "multiplicacion":
                oper = numero1 * numero2 ;
                Console.WriteLine("La multiplicacion de "+ numero1 +" por " + numero2 +" es igual a " + oper);
				break;
			case "division":
                oper = numero1 / numero2 ;
                Console.WriteLine("La division de "+ numero1 +" entre " + numero2 +" es igual a " + oper);
				break;
		}
	}
	public void actividad5()
	{
		/*3. Programa para determinar el área o el perímetro de un triángulo ó un cuadrado, o un rectángulo o un círculo,
		las medidas y lo que quiere calcular debe ser introducido por el usuario. (Usar Switch)*/
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 5                             +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine(" ");
		string opcion = "";
		string poa = "";
		double pi ; 
		double oper3, rad ;
		int oper2;
		int bas, bas2, bas3, alt;
		Console.WriteLine("Se calculara el area y perimtro de una figura");
		Console.WriteLine("< triangulo >    < cuadrado >    < rectangulo >    < circulo >");
		Console.WriteLine(" ");
		
		Console.WriteLine("Escribe el nombre de la figura a trabajar");
		opcion = Console.ReadLine();
        Console.WriteLine("Que desea calcular, el area o perimetro?");
		poa = Console.ReadLine();
		
        switch(poa)
		{
			case "area":
				switch(opcion)
		        {
			        case "triangulo":
						Console.WriteLine("Ingrese la base del triangulo: ");
		                bas = Int32.Parse(Console.ReadLine());
		
		                Console.WriteLine("Ingrese la altura del triangulo: ");
		                alt = Int32.Parse(Console.ReadLine());
				        oper2 = (bas * alt)/2 ;
                        Console.WriteLine("El area del triangulo es igual a "+ bas +" por " + alt +" /2 = " + oper2);
				    break;
			        case "cuadrado":
						Console.WriteLine("Ingrese la medida de un lado del cuadrado: ");
		                bas = Int32.Parse(Console.ReadLine());
						oper2 = (bas * bas)/2 ;
                        Console.WriteLine("El area del cuadrado es igual a "+ bas +" por " + bas +" = " + oper2);
				    break;
		            case "rectangulo":
						Console.WriteLine("Ingrese el lado menor del rectangulo: ");
		                bas = Int32.Parse(Console.ReadLine());
		
		                Console.WriteLine("Ingrese el lado mayor del rectangulo: ");
		                alt = Int32.Parse(Console.ReadLine());
				        oper2 = bas * alt ;
                        Console.WriteLine("El area del rectangulo es igual a "+ bas +" por " + alt +" = " + oper2);
                    
				    break;
			        case "circulo":
						Console.WriteLine("Ingrese el radio del circulo: ");
		                rad = Int32.Parse(Console.ReadLine());
						pi = 3.1416;
						oper3 = (pi * rad) ;
						oper3 = oper3*oper3;
                        Console.WriteLine("El area del circulo es igual a " + oper3);
				    break;
		        }
				break;
			case "perimetro":
                switch(opcion)
		        {
			        case "triangulo":
						Console.WriteLine("Ingrese la base del triangulo: ");
		                bas = Int32.Parse(Console.ReadLine());
						Console.WriteLine("Ingrese la base del triangulo: ");
		                bas2 = Int32.Parse(Console.ReadLine());
						Console.WriteLine("Ingrese la base del triangulo: ");
		                bas3 = Int32.Parse(Console.ReadLine());
		
				        oper2 = bas + bas2 + bas3 ;
                        Console.WriteLine("El perimetro del triangulo es igual a " + oper2);
				    break;
			        case "cuadrado":
						Console.WriteLine("Ingrese la medida de un lado del cuadrado: ");
		                bas = Int32.Parse(Console.ReadLine());
						oper2 = bas*4 ;
                        Console.WriteLine("El perimetro del cuadrado es igual a " + oper2);
				    break;
		            case "rectangulo":
						Console.WriteLine("Ingrese el lado menor del rectangulo: ");
		                bas = Int32.Parse(Console.ReadLine());
		
		                Console.WriteLine("Ingrese el lado mayor del rectangulo: ");
		                alt = Int32.Parse(Console.ReadLine());
				        oper2 = bas*2 + alt*2 ;
                        Console.WriteLine("El perimetro del rectangulo es igual a " + oper2);
                    
				    break;
			        case "circulo":
						Console.WriteLine("Ingrese el radio del circulo: ");
		                rad = Int32.Parse(Console.ReadLine());
						pi = 3.1416;
						oper3 = 2 * pi * rad ;
                        Console.WriteLine("El perimetro del circulo es igual a " + oper3);
				    break;
		        }
				break;
		}
	}
