	public void actividad7()
	{
		//1.- Muestre en pantalla los números del 1 al 100 en orden ascendente
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 7                             +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int x;
		for(x = 1; x<=100; x++)
		{
			Console.WriteLine("Repeticion # " + x);
		}
	}
	public void actividad8()
	{
		//2.- Muestre los números del 100 al 0 en orden descendente 
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 8                             +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int y;
		for(y = 100; y>=1; y--)
		{
			Console.WriteLine("Repeticion # " + y);
		}
	}
	public void actividad9()
	{
		//3.- Imprimir la suma de los primero 100 numeros 
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 9                             +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int n;
		int sum = 0;
		for(n = 1; n<=100 ; n++)
		{
			sum = sum + n;
			Console.WriteLine("Suma de : " +(n-1) +" mas "+ (sum-(n-1)) +" es igual a " +sum);
		}
		Console.WriteLine("La suma de los primeros 100 números es de: " + sum);
	}
	public void actividad10()
	{
		//4.- Introducir tantas frases como el usuario quiera y contar cuantas fueron.
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 10                            +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		string frase;
		int n1 = 0;
		Console.WriteLine("Ingresa las fraces que guste");
		Console.WriteLine("Cuando desee terminar solo esbriba la palabra <finalizar>");
		for(frase = ""; frase != "finalizar"; frase = Console.ReadLine())
		{
			n1= n1 + 1;
		}
		Console.WriteLine("El numero de frases ingresadas es de: " + (n1-1));
	}
	public void actividad11()
	{
		//5.- Introducir dos numeros por teclado. Imprimir los numeros naturales que hay entre ambos numeros empezando por el más pequeño, contar cuantos hay y cuantos de ellos son pares. Calcular la suma de los impares.
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 11                            +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int a,b, e = 0, a1, b1;
		int c = 1;
		int d = 1, q1 = 0;
		int suma = 0, sumapar = 0;
		int par = 0;
		int sumaimpar = 0;
		Console.WriteLine("Ingrese dos números: ");
		a = Int32.Parse(Console.ReadLine());
		b = Int32.Parse(Console.ReadLine());
		
		for(c = 1  ; (a>=0 && b>=0 && c<=1) ; c++ )
		{
			for(d = 1 ; (a<b && d<=1) ; d++)
			{
				a = a + 1;
				b = b - 1;
				a1 = a ;
				b1 = b ;
				for( a = a1 ; (a<=b) ; a++ )
				{
					Console.WriteLine("#" + a);
					for(par = 0 ; (a % 2 == 0 && par < 1) ; par ++ )
					{
						sumapar = sumapar + 1;
					}
					par = 0 ;
					for(par = 0 ; (a % 2 == 1 && par < 1) ; par ++ )
					{
						sumaimpar = sumaimpar + 1;
						e = e + a ;
					}
					par = 0 ;
					suma = suma + 1;
					
				}
				Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (suma) + " números");
				Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (sumapar) + " números pares");
				Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (sumaimpar) + " números impares");
				Console.WriteLine("La suma de los números impares es de: " + e);
				q1 = d ;
			}
			for(d = q1 ; (a>b && d<1) ; d ++ )
			{
				a = a - 1;
				b = b + 1;
				a1 = a ;
				b1 = b ;
				for( b = b1 ; (b<=a) ; b ++ )
				{
					for(par = 0 ; (b % 2 == 0 && par < 1) ; par ++ )
					{
						sumapar = sumapar + 1;
					}
					par = 0 ;
					for(par = 0 ; (b % 2 == 1 && par < 1) ; par ++ )
					{
						sumaimpar = sumaimpar + 1;
						e = e + b ;
					}
					par = 0 ;
					Console.WriteLine("#" + b);
					suma = suma + 1;
				}
				Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (suma) + " números");
				Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (sumapar) + " números pares");
				Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (sumaimpar) + " números impares");
				Console.WriteLine("La suma de los números impares es de: " + e);
			}
		}
		for(c = 1 ; ((a<=0 || b<=0) && c<=1) ; c++ )
		{
			Console.WriteLine("El o los numeros ingresados son negativos");	
		}
		for(e = 0 ; (a == b && e<1) ; e++ )
		{
			Console.WriteLine("Los números ingresados son iguales");
		}
	}
	public void actividad12()
	{
		//8. Algoritmo que muestre una lista de todos los números pares que hay entre dos números cualquiera y que nos
        //diga cuantos son. (Ejemplo: Se leen los números 8 y 17, el resultado del algoritmo debe ser 10,12,14,16, Entre
        //el 8 y el 17 hay 5 números pares)
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 12                            +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int g1, g2, g3 = 0, g4 = 0, g5 = 0 ;
		int g6, g7 ;
		Console.WriteLine("Ingrese dos números: ");
		g1 = Int32.Parse(Console.ReadLine());
		g2 = Int32.Parse(Console.ReadLine());
		g6 = g1 ;
		g7 = g2 ;
		while(g1 < g2 && g3 < 1)
		{
			g1 = g1 + 1;
			g2 = g2 - 1;
			while(g1 <= g2 )
			{
				while(g1 % 2 == 0 && g4 < 1 )
				{
					Console.WriteLine("#: " + g1);
					g4 ++ ;
					g5 = g5 + 1 ;
				}
				g4 = 0 ;
				g1++;
			}
			g3 ++ ;
			Console.WriteLine("Entre el " + g6 + " y el " + g7 + " hay " + g5 + " números pares.");
		}
		while(g2 < g1 && g3 < 1)
		{
			g1 = g1 - 1;
			g2 = g2 + 1;
			while(g2 <= g1 )
			{
				while(g2 % 2 == 0 && g4 < 1 )
				{
					Console.WriteLine("#: " + g2);
					g4 ++ ;
					g5 = g5 + 1 ;
				}
				g4 = 0 ;
				g2++;
			}
			g3 ++ ;
			Console.WriteLine("Entre el " + g7 + " y el " + g6 + " hay " + g5 + " números pares.");
		}
		while(g1 == g2 && g3<1)
		{
			Console.WriteLine("Los números ingresados son iguales");
			g3 ++ ;
		}
	}
	public void actividad13()
	{
		//9. Algoritmo que muestre una lista de todos los números impares que hay entre dos números cualquiera. (Ejemplo:
        //Se leen los números 8 y 17, el resultado del algoritmo debe ser 9,11,13,15, Entre el 8 y el 17 hay 4 números
        //impares)
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 13                            +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int h1, h2, h3 = 0, h4 = 0, h5 = 0 ;
		int h6, h7 ;
		Console.WriteLine("Ingrese dos números: ");
		h1 = Int32.Parse(Console.ReadLine());
		h2 = Int32.Parse(Console.ReadLine());
		h6 = h1 ;
		h7 = h2 ;
		while(h1 < h2 && h3 < 1)
		{
			h1 = h1 + 1;
			h2 = h2 - 1;
			while(h1 <= h2 )
			{
				while(h1 % 2 == 1 && h4 < 1 )
				{
					Console.WriteLine("#: " + h1);
					h4 ++ ;
					h5 = h5 + 1 ;
				}
				h4 = 0 ;
				h1++;
			}
			h3 ++ ;
			Console.WriteLine("Entre el " + h6 + " y el " + h7 + " hay " + h5 + " números impares.");
		}
		while(h2 < h1 && h3 < 1)
		{
			h1 = h1 - 1;
			h2 = h2 + 1;
			while(h2 <= h1 )
			{
				while(h2 % 2 == 1 && h4 < 1 )
				{
					Console.WriteLine("#: " + h2);
					h4 ++ ;
					h5 = h5 + 1 ;
				}
				h4 = 0 ;
				h2++;
			}
			h3 ++ ;
			Console.WriteLine("Entre el " + h7 + " y el " + h6 + " hay " + h5 + " números impares.");
		}
		while(h1 == h2 && h3<1)
		{
			Console.WriteLine("Los números ingresados son iguales");
			h3 ++ ;
		}
	}
	public void actividad14()
	{
		//10. Algoritmo para leer 5 números, obtener su promedio, el mayor y el menor de esos números.
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 14                            +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int c1, c2, c3, c4, c5, promedio;
		int x1 = 0, x2 = 0;
		Console.WriteLine("Ingrese cinco números: ");
		c1 = Int32.Parse(Console.ReadLine());
		c2 = Int32.Parse(Console.ReadLine());
		c3 = Int32.Parse(Console.ReadLine());
		c4 = Int32.Parse(Console.ReadLine());
		c5 = Int32.Parse(Console.ReadLine());
		promedio = (c1 + c2 + c3 + c4 + c5 )/5;
		Console.WriteLine("El promedio de los 5 números ingresados es de:" + promedio);
		
		while(c1==c2 && c1==c3 && c1==c4 && c1==c5 && x1<1 && x2<1)
		{
			Console.WriteLine("Todos los números ingresados son iguales");
			x1 ++ ;
			x2 ++ ;
		}
		while((c1>c2 || c1>=c2) && (c1>c3 || c1>=c3) && (c1>c4 || c1>=c4) && (c1>c5 || c1>=c5) && (x1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c1+" que fue el primer numero en ser ingresado");
			x1 ++ ;
		}
		while((c2>c1 || c2>=c1) && (c2>c3 || c2>=c3) && (c2>c4 || c2>=c4) && (c2>c5 || c2>=c5) && (x1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c2+" que fue el segundo numero en ser ingresado");
			x1 ++ ;
		}
		while((c3>c2 || c3>=c2) && (c3>c1 || c3>=c1) && (c3>c4 || c3>=c4) && (c3>c5 || c3>=c5) && (x1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c3+" que fue el tercer numero en ser ingresado");
			x1 ++ ;
		}
		while((c4>c2 || c4>=c2) && (c4>c3 || c4>=c3) && (c4>c1 || c4>=c1) && (c4>c5 || c4>=c5) && (x1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c4+" que fue el cuarto numero en ser ingresado");
			x1 ++ ;
		}
		while((c5>c2 || c5>=c2) && (c5>c3 || c5>=c3) && (c5>c4 || c5>=c4) && (c5>c1 || c5>=c1) && (x1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c5+" que fue el quinto numero en ser ingresado");
			x1 ++ ;
		}
		while((c1<c2 || c1<=c2) && (c1<c3 || c1<=c3) && (c1<c4 || c1<=c4) && (c1<c5 || c1<=c5) && (x2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c1+" que fue el primer numero en ser ingresado");
			x2 ++ ;
		}
		while((c2<c1 || c2<=c1) && (c2<c3 || c2<=c3) && (c2<c4 || c2<=c4) && (c2<c5 || c2<=c5) && (x2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c2+" que fue el segundo numero en ser ingresado");
			x2 ++ ;
		}
		while((c3<c2 || c3<=c2) && (c3<c1 || c3<=c1) && (c3<c4 || c3<=c4) && (c3<c5 || c3<=c5) && (x2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c3+" que fue el tercer numero en ser ingresado");
			x2 ++ ;
		}
		while((c4<c2 || c4<=c2) && (c4<c3 || c4<=c3) && (c4<c1 || c4<=c1) && (c4<c5 || c4<=c5) && (x2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c4+" que fue el cuarto numero en ser ingresado");
			x2 ++ ;
		}
		while((c5<c2 || c5<=c2) && (c5<c3 || c5<=c3) && (c5<c4 || c5<=c4) && (c5<c1 || c5<=c1) && (x2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c5+" que fue el quinto numero en ser ingresado");
			x2 ++ ;
		}
	}
	public void actividad15()
	{
		//11. Algoritmo “Criba de Eratóstenes”, para saber los números primos que hay entre dos números. (Consulte anexos
        //en la Tarea 5)
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 15                            +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int divisible = 0 ;
		int inicio ;
		int p1 = 0 , p2 = 0 , p3 = 0;
		Console.WriteLine("Ingrese un número: ");
		inicio = Int32.Parse(Console.ReadLine());
		Console.WriteLine("Ingresa hasta que número quieras que imprima los números primos");
		int numero_final = int.Parse(Console.ReadLine());
		while( inicio == numero_final && p2<1)
		{
			Console.WriteLine("Los numeros ingresados son iguales");
			p2 ++ ;
		}
		inicio = inicio + 1;
		numero_final = numero_final - 1;
		while(inicio < numero_final)
		{
			int i = 1;
			while( i <= inicio)
			{
				while(inicio % i == 0 && p1<1)
				{
					divisible ++ ;
					p1 ++;
				}
				p1 = 0 ;
				while(divisible > 2)
				{
					break;
				}
				p2 = 0 ;
				i ++ ;
			}
			while(divisible == 2 && p3<1)
			{
				Console.WriteLine("#" + inicio + " es primo");
				p3++;
			}
			p3 = 0 ;
			divisible = 0 ;
			inicio ++ ;
		}
		inicio = inicio - 1;
		numero_final = numero_final + 1;
		while(numero_final < inicio)
		{
			int i = 1;
			while( i <= numero_final)
			{
				while(numero_final % i == 0 && p1<1)
				{
					divisible ++ ;
					p1 ++;
				}
				p1 = 0 ;
				while(divisible > 2)
				{
					break;
				}
				p2 = 0 ;
				i ++ ;
			}
			while(divisible == 2 && p3<1)
			{
				Console.WriteLine("#" + numero_final + " es primo");
				p3++;
			}
			p3 = 0 ;
			divisible = 0 ;
			numero_final ++ ;
		}
	}
