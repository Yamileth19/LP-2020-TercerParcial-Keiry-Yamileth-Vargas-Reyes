	public void actividad17()
	{
		//1.- Desarrollar un programa que permita cargar 5 nombres de personas y sus edades respectivas.
		int k1 = 0, k2=0;
		string opcion = "";
		int k=0;
		int[] años = new int[5];
		string[] nombre = new string[5];
		Console.WriteLine("==============================================================");
		Console.WriteLine("========================= PROGRAMA 17 =========================");
		Console.WriteLine("==============================================================");
		for(int d=0; d<5; d++){
			Console.Write("Ingrese el nombre de la persona seleccionada "+ (d+1) + ": ");
			nombre[d] = Console.ReadLine();
			Console.Write("Ingrese los años de la persona seleccionada "+ (d+1) + ": ");
			años[d] = int.Parse(Console.ReadLine());
		}
		Console.Clear();
		while(k<1){
			k1=0;
			k++;
			Console.WriteLine("===========================================================");
			Console.WriteLine("                        Menú"                               );
			Console.WriteLine(" ");
		    Console.WriteLine(" A) Los nombres y edades de las personas mayores de edad en México.");
		    Console.WriteLine(" B) Los nombres y edades de las personas mayores de edad en Estados Unidos.");
			Console.WriteLine(" C) Los nombres y edades de las personas menores de edad en México.");
		    Console.WriteLine(" D) Los nombres y edades de las personas menores de edad en Estados Unidos");
		    Console.WriteLine(" E) Salir ");
		    Console.WriteLine(" ");
		    Console.WriteLine("	Ingresa la letra del inciso que deseas consultar");
		    Console.WriteLine(" ");
			opcion = Console.ReadLine();
			switch(opcion){
				case "A":
					Console.Clear();
					Console.WriteLine("=======================================================================");
			        Console.WriteLine(" A) Los nombres y edades de las personas mayores de edad en México.");
			        Console.WriteLine(" ");
					for(int j=0;j<5 ; j++){
			            while(años[k1]>=18 && k2<1){
			                Console.WriteLine("El nombre de la persona " + (k1+1) + " es: "+ nombre[k1]);
			                Console.WriteLine("La edad de la persona " + (k1+1) + " es: "+ años[k1] + " años");
				            k2++;
			            }
			            k2 = 0;
			            k1++;
		            }
					k = 0;
					break;
				case "B":
					Console.Clear();
					Console.WriteLine("================================================================================");
			        Console.WriteLine(" B) Los nombres y edades de las personas mayores de edad en Estados Unidos.");
			        Console.WriteLine(" ");
					for(int j=0;j<5 ; j++){
			            while(años[k1]>=21 && k2<1) {
			                Console.WriteLine("El nombre de la persona " + (k1+1) + " es: "+ nombre[k1]);
			                Console.WriteLine("La edad de la persona " + (k1+1) + " es: "+ años[k1] + " años");
				            k2++;
			            }
			            k2 = 0;
			            k1++;
		            }
					k = 0;
					break;
				case "C":
					Console.Clear();
					Console.WriteLine("=======================================================================");
			        Console.WriteLine(" C) Los nombres y edades de las personas menores de edad en México.");
			        Console.WriteLine(" ");
					for(int j=0;j<5 ; j++){
			            while(años[k1]<18 && k2<1){
			                Console.WriteLine("El nombre de la persona " + (k1+1) + " es: "+ nombre[k1]);
			                Console.WriteLine("La edad de la persona " + (k1+1) + " es: "+ años[k1] + " años");
				            k2++;
			            }
			            k2 = 0;
			            k1++;
		            }
					k = 0;
					break;
				case "D":
					Console.Clear();
					Console.WriteLine("==============================================================================");
			        Console.WriteLine(" D) Los nombres y edades de las personas menores de edad en Estados Unidos");
			        Console.WriteLine(" ");
					for(int j=0;j<5 ; j++){
			            while(años[k1]<21 && k2<1){
			                Console.WriteLine("El nombre de la persona " + (k1+1) + " es: "+ nombre[k1]);
			                Console.WriteLine("La edad de la persona " + (k1+1) + " es: "+ años[k1] + " años");
				            k2++;
			            }
			            k2 = 0;
			            k1++;
		            }
					k = 0;
					break;
				case "E":
					Console.Clear();
					Console.WriteLine("Ha salido del problema 1");
					break;

				default:
					Console.Clear();
					Console.WriteLine("La letra que ingreso no es valida");
					k = 0;
					break;
			}
		}
		
	}
	public void actividad18()
	{
		//2.- Desarrollar un programa que primero nos permita capturar n números, y después nos permita buscar un elemento. 
		//Al final nos muestre como resultado si encontró el número buscado y en qué posición se encuentra.
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 18                            +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		int num = 0;
		int con = 0;
		Console.Write("¿Cuántos números desea ingresar?: ");
		num = int.Parse(Console.ReadLine());
		int[] crack = new int[num];
		for(int b = 0; b < num; b++){
			Console.Write(""+(b+1) + " Ingrese un número: ");
			crack[b] = int.Parse(Console.ReadLine());
		}
		Console.WriteLine("====================================================================");
		Console.WriteLine("De los " + num + " números, ¿Qué numero desea consultar en este momento?");
		con = int.Parse(Console.ReadLine());
		Console.WriteLine("====================================================================");
		Console.WriteLine("Calificación " + con + " : "+ crack[con-1]);
	}	
	public void actividad19()
	{
		//3.- Desarrollar un programa que capture valores de dos matrices de 3x3
		string letra = "";
		int v=0, v1=0;
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		Console.WriteLine("+                             PROGRAMA 19                            +");
		Console.WriteLine("++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++");
		//Declaracion de matriz/
		int[,] matriz1 = new int[3,3];
		int[,] matriz2 = new int[3,3];
		int[,] res = new int[3,3];
		//Guardar informacion de la matriz 1,2,3/
		Console.WriteLine("Matriz 1");
		for(int fila = 0; fila  <= 2; fila++){
			for(int columna = 0; columna <= 2; columna++){
				//Pasar el valor a nuestra matriz/
				Console.Write("["+fila+","+columna+"]" + "Ingrese un número entero: ");
				matriz1[fila,columna] = int.Parse(Console.ReadLine());
			}
		}
		Console.WriteLine("Matriz 2");
		for(int fila = 0; fila  <= 2; fila++){
			for(int columna = 0; columna <= 2; columna++){
				//Pasar el valor a nuestra matriz/
				Console.Write("["+fila+","+columna+"]" + "Ingrese un número entero: ");
				matriz2[fila,columna] = int.Parse(Console.ReadLine());
			}
		}
		Console.Clear();
		while(v<1){
			v++;
			Console.WriteLine("            MENU");
		    Console.WriteLine("a) Mostrar el resultado de la suma de las matrices");
		    Console.WriteLine("b) Mostrar el resultado de la resta de las matrices");
		    Console.WriteLine("c) Mostrar el resultado de la multiplicación de las matrices");
		    Console.WriteLine("d) Salir");
		    Console.WriteLine(" ");
		    Console.WriteLine("Ingresee la letra del inciso que desea consultar");
		    Console.WriteLine(" ");
	        letra = Console.ReadLine();
			switch(letra){
				case "a":
					Console.Clear();
					Console.WriteLine("==================================");
		            Console.WriteLine("SUMA DE LAS MATRICES");
		            Console.WriteLine("==================================");
					for(int i=0 ; i<3 ; i++){
			            for(int j=0 ; j<3; j++){
				            res[i,j] = matriz1[i,j]+ matriz2[i,j];
			            }
		            }
					v=0;
					break;
				case "b":
					Console.Clear();
					Console.WriteLine("==================================");
		            Console.WriteLine("RESTA DE LAS MATRICES");
		            Console.WriteLine("==================================");
					Console.WriteLine("tu ");
					for(int i=0 ; i<3 ; i++){
			            for(int j=0 ; j<3; j++){
				            res[i,j] = matriz1[i,j] - matriz2[i,j];
			            }
		            }
					v=0;
					break;
				case "c":
					Console.Clear();
					Console.WriteLine("==================================");
		            Console.WriteLine("MULTIPLICACIÓN DE LAS MATRICES");
		            Console.WriteLine("==================================");
					Console.WriteLine("el ");
					for(int i=0 ; i<3 ; i++){
			            for(int j=0 ; j<3; j++){
				            res[i,j] = matriz1[i,j] * matriz2[i,j];
			            }
		            }
					v=0;
					break;
				case "d":
					Console.Clear();
					Console.WriteLine("Ha salido del problema 3");
					 v1=2;
					break;
				default:
					Console.Clear();
					Console.WriteLine("La letra que ingreso no es valida");
					v=0; v1=2;
					break;
			}
			//RESULTADOS
			for(int fila = 0; fila  <= 2 && v1<1; fila++){
			    for(int columna = 0; columna <= 2; columna++){
				//Debe de imprimir los valores de la matriz
				    Console.Write(res[fila,columna]+" | ");
			    }
			    Console.WriteLine("");
			    Console.WriteLine("------------");
		    }v1=0;
		}
	}
